{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard",
        "plugin:react/jsx-runtime"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "comma-dangle": [
            "error",
            "always-multiline"
        ],
        "semi": [
            "warn",
            "always"
        ],
        "no-unused-vars": [
            "warn",
            {
                "varsIgnorePattern": "VARIABLE_NAME"
            }
        ],
        "quotes": [
            "error",
            "single",
            "backtick"
        ]
        // "quotes": [
        //     2,
        //     "single",
        //     {
        //         "avoidEscape": true,
        //         "allowTemplateLiterals": true
        //     }
        // ]
        // "eol-last": [
        //     "warn",
        //     "never"
        // ]
    }
}